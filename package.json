{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start:dev": "ts-node-dev --respawn ./src/server.ts",
    "watch:dev": "nodemon ./dist/server.js",
    "start": "node ./dist/server.js",
    "clean": "rimraf dist/*",
    "tsc-watch": "tsc --watch --preserveWatchOutput",
    "tsc": "tsc",
    "clear:test": "jest --clearCache",
    "test": "jest --detectOpenHandles --config ./jest.config.js",
    "test:dev": "jest --detectOpenHandles --watchAll",
    "prettier": "prettier \"**/*.+(ts|js|json)\"",
    "format": "npm run prettier -- --write",
    "watch": "npm-run-all clean tsc-watch",
    "heroku-deploy": "git push heroku master",
    "seed": "sequelize db:seed",
    "seed:all": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "migrate": "sequelize db:migrate",
    "migrate:all": "sequelize db:migrate:all",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all"
  },
